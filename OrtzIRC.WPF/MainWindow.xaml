<Window x:Class="OrtzIRC.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:OrtzIRC.WPF"
        xmlns:vm="clr-namespace:OrtzIRC.WPF.ViewModels"
        Title="MainWindow" Height="423" Width="769" x:Name="window">
    <DockPanel LastChildFill="True">
        <TabControl x:Name="tabControl">
            <TabItem>
                <local:ChannelPanel>
                    <local:ChannelPanel.DataContext>
                        <vm:ChannelViewModel />
                    </local:ChannelPanel.DataContext>
                </local:ChannelPanel>
            </TabItem>
        </TabControl>
    </DockPanel>
</Window>
