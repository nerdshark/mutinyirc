<UserControl x:Class="OrtzIRC.WPF.ChannelPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OrtzIRC.WPF"
             mc:Ignorable="d" 
             d:DesignHeight="169" d:DesignWidth="277" x:Name="channelPanel">
    <DockPanel LastChildFill="True">
        <local:CommandTextBox DockPanel.Dock="Bottom" x:Name="commandBox" MinHeight="20" CommandEntered="commandBox_CommandEntered" />
        <local:IrcOutputBox x:Name="outputBox" />
    </DockPanel>
</UserControl>
